        "editor.defaultFormatter": "ms-python.autopep8",
        "editor.formatOnSave": true,
        "editor.rulers": [
            140
        ],
    },
    "autopep8.args": [
        "--max-line-length=140",
        "--aggressive",
        "--aggressive"
    ],
