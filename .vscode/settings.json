        "activityBarBadge.background": "#e8b397",
        "activityBarBadge.foreground": "#15202b",
        "commandCenter.border": "#e7e7e799",
        "sash.hoverBorder": "#8e64dc",
        "statusBar.background": "#703ad3",
        "statusBar.foreground": "#e7e7e7",
        "statusBarItem.hoverBackground": "#8e64dc",
        "statusBarItem.remoteBackground": "#703ad3",
        "statusBarItem.remoteForeground": "#e7e7e7",
        "titleBar.activeBackground": "#703ad3",
        "titleBar.activeForeground": "#e7e7e7",
